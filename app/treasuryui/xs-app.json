{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    
    {
      "source": "^/browsebooks/(.*)$",
      "target": "/$1",
      "destination": "treasury_browsebooks",
      "authenticationType": "none"
    },
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "destination": "treasuryUpload",
      "authenticationType": "none"
    },
    {
      "source": "^/v2/odata/v4/Treasury-srv-api/(.*)$",
      "target": "/v2/odata/v4/Treasury-srv-api/$1",
      "authenticationType": "xsuaa",
      "destination": "treasuryUpload"
    },

    {
      "source": "^/odata/v4/Treasury-srv-api/(.*)$",
      "target": "/odata/v4/Treasury-srv-api/$1",
      "authenticationType": "xsuaa",
      "destination": "treasuryUpload"
    },
    {
      "source": "^/user-api/currentUser$",
      "target": "/currentUser",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/odata/v4/catalog/(.*)$",
      "authenticationType": "none",
      "destination": "Treasury-srv-api"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
