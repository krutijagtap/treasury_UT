{
  "xsappname": "Treasury",
  "tenant-mode": "dedicated",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
      "name": "$XSAPPNAME.Support",
      "description": "Support"
    },
    {
      "name": "$XSAPPNAME.ContentChecker",
      "description": "ContentChecker"
    },
    {
      "name": "$XSAPPNAME.ContentMaker",
      "description": "ContentMaker"
    }
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA Token Exchange",
      "scope-references": ["uaa.user"]
    },
    {
      "name": "Workzone_EFDNA_GenAI_Treasury_Support",
      "description": "Support",
      "scope-references": ["$XSAPPNAME.Support"]
    },
    {
      "name": "Workzone_EFDNA_GenAI_Treasury_Checker",
      "description": "ContentChecker",
      "scope-references": ["$XSAPPNAME.ContentChecker"]
    },
    {
      "name": "Workzone_EFDNA_GenAI_Treasury_Maker",
      "description": "ContentMaker",
      "scope-references": ["$XSAPPNAME.ContentMaker"]
    }
  ],
  "oauth2-configuration": {
    "token-validity": 3600,
    "redirect-uris": [
      "https://*-Treasury-app.cfapps.*.hana.ondemand.com/login/callback",
      "https://*.cfapps.*.ondemand.com/**",
      "https://*.cfapps.*.hana.ondemand.com/**"
    ]
  }
}
